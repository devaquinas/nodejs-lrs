{
"language": "coffeescript",
	"views": {
		"email": {
			"map": "(doc) ->\n  emit doc.actor.mbox.toString(), doc"
		},
		"verb": {
			"map": "(doc) ->\n  emit doc.verb, doc"
		},
		"activity_id": {
			"map": "(doc) ->\n  emit doc.object.id, doc"
		},
		"timestamp": {
			"map": "(doc) ->\n  emit doc.timestamp, doc"
		},
		"stored": {
			"map": "(doc) ->\n  emit doc.stored, doc"
		}
	}
}
